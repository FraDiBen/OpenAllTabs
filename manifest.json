{
  "name": "Open All the Tabs!",
  "version": "0.0.1",
  "description": "Transfer and Open tabs from different devices",
  "permissions": ["storage", "tabs", "sessions"],
  "background": {
    "scripts": ["script.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Open All the Tabs!",
    "default_icon": "img/icon_generic.png"
  },
  "manifest_version": 2
}