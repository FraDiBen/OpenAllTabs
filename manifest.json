{
  "name": "Open All the Tabs!",
  "version": "0.0.2",
  "description": "Transfer and Open tabs from different devices",
  "permissions": ["tabs", "sessions"],
  // "background": {
  //   "scripts": ["back.js"],
  //   "persistent": false
  // },
  "browser_action": {
    "default_title": "Open All the Tabs!",
    "default_icon": "img/transfer.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/transfer.png",
    "48": "img/transfer.png",
    "128": "img/transfer.png" 
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}